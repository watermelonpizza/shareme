{
  "HostUrl": "http://localhost:5000",
  // Folder name for the files to be uploaded into
  "UploadFolder": "files",
  // Path after the host url that the files will be served from 
  // Must start with a '/', can be blank to server to root (http://localhost/abc123.png)
  "FileRequestPath": "/f",
  "AdminKey": "adminkey",
  "ExtensionBlacklist": [ "sh", "exe", "bat" ],
  "MaxUploadSizeInBytes": 60000000,
  "CloudFlareZone": null,
  "CloudFlareEmail": null,
  "CloudFlareKey": null,
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
